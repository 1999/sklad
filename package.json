{
  "name": "sklad",
  "version": "4.1.1",
  "description": "Promise-based IndexedDB thin abstraction layer",
  "directories": {
    "doc": "docs"
  },
  "main": "lib/sklad.js",
  "scripts": {
    "test": "./node_modules/karma/bin/karma start --single-run",
    "release": "npm run release:dev && npm run release:prod",
    "release:dev": "NODE_ENV=development ./node_modules/.bin/webpack",
    "release:prod": "NODE_ENV=production ./node_modules/.bin/webpack",
    "watch": "IS_DEVELOPMENT_PROCESS=1 ./node_modules/.bin/webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/1999/sklad.git"
  },
  "keywords": [
    "indexeddb",
    "html5",
    "database"
  ],
  "author": "Dmitry Sorin <info@staypositive.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1999/sklad/issues"
  },
  "homepage": "https://github.com/1999/sklad",
  "dependencies": {
    "kinopromise": "^0.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-es2015-typeof-symbol": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-jasmine": "^0.3.8",
    "karma-mocha-reporter": "^2.0.0",
    "karma-sauce-launcher": "^0.3.1",
    "promise-polyfill": "^4.0.3",
    "webpack": "^1.12.14"
  }
}
