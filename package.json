{
  "name": "sklad",
  "version": "4.2.4",
  "description": "Promise-based API for IndexedDB",
  "directories": {
    "doc": "docs"
  },
  "main": "dist/sklad.uncompressed.js",
  "jsnext:main": "lib/sklad.js",
  "scripts": {
    "clean": "rm -fr node_modules/ dist/",
    "lint": "./node_modules/.bin/eslint lib/ tests/",
    "test": "yarn prepublish && ./node_modules/karma/bin/karma start --single-run",
    "prepublish": "yarn release:dev && yarn release:prod",
    "release:dev": "NODE_ENV=development ./node_modules/.bin/webpack --mode development",
    "release:prod": "NODE_ENV=production ./node_modules/.bin/webpack --mode production",
    "watch": "./node_modules/.bin/webpack --mode development --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/1999/sklad.git"
  },
  "keywords": [
    "indexeddb",
    "html5",
    "database"
  ],
  "author": "Dmitrii Sorin <info@staypositive.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1999/sklad/issues"
  },
  "homepage": "https://github.com/1999/sklad",
  "devDependencies": {
    "ajv": "^6.2.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-add-module-exports": "^0.3.1",
    "babel-plugin-transform-es2015-typeof-symbol": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.0.0",
    "eslint-plugin-jasmine": "^2.7.1",
    "jasmine-core": "^3.2.0",
    "karma": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sauce-launcher": "^1.1.0",
    "pre-commit": "^1.2.2",
    "promise-polyfill": "^8.0.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.0.0"
  },
  "pre-commit": "lint"
}
